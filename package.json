{
  "name": "IncrudiblePeople",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000/api/v1",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "npm-sass": "^2.0.0",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-responsive-modal": "^1.4.1",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
